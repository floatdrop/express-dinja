    "injection"
  ],
  "files": [
    "index.js",
    "utils.js"
  ],
  "author": "Vsevolod Strukchinsky <floatdrop@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/floatdrop/express-dinja/issues"
  },
  "homepage": "https://github.com/floatdrop/express-dinja",
  "devDependencies": {
    "coveralls": "^2.11.6",
    "express": "^4.13.4",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.0.0",
    "supertest": "^1.1.0",
    "xo": "^0.12.1"
  },
  "dependencies": {
    "async": "^1.5.2",
    "dag": "^0.0.1",
